#: data/louper.appdata.xml.in:7 data/louper.desktop.in:3
msgid "Louper"
msgstr "Louper"

#: data/louper.appdata.xml.in:8 data/louper.desktop.in:5
msgid "Magnify the copied text"
msgstr "コピーしたテキストを拡大します"

#: data/louper.appdata.xml.in:10
msgid "A text magnification app that helps you zoom the copied text."
msgstr "コピーしたテキストを拡大するのに役立つ、テキスト拡大アプリです。"

#: data/louper.appdata.xml.in:13
msgid "Workflow:"
msgstr "手順:"

#: data/louper.appdata.xml.in:15
msgid "Copy some text"
msgstr "テキストをコピーします"

#: data/louper.appdata.xml.in:16
msgid "Launch the app with Super+Q"
msgstr "Super+Q キーを押して、アプリを起動します"

#: data/louper.appdata.xml.in:17
msgid "Press Ctrl+C to copy the text showing"
msgstr "Ctrl+C キーを押すと、表示されたテキストをコピーできます"

#: data/louper.appdata.xml.in:18
msgid "Press Esc or unfocus the window to close the app"
msgstr "Esc キーを押すかウィンドウを非アクティブにすると、アプリを終了します"

#: data/louper.appdata.xml.in:61
msgid "Initial release"
msgstr "最初のリリース"

#: data/louper.appdata.xml.in:67
msgid "Ryo Nakano"
msgstr "Ryo Nakano"

#: data/louper.desktop.in:4
msgid "Text Magnification App"
msgstr "テキスト拡大アプリ"

#: data/louper.desktop.in:8
msgid "com.github.ryonakano.louper"
msgstr "com.github.ryonakano.louper"

#: data/louper.desktop.in:11
